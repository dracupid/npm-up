  "dependencies": {
    "chalk": "latest",
    "cli-spinner": "^0.2.10",
    "commander": "^2.19.0",
    "global-npm": "latest",
    "kiss-request": "^1.0.0",
    "nofs": "0.12.2",
    "semver": "latest",
    "underscore": "^1.12.1"
  },
  "devDependencies": {
    "coffeelint": "2.1.0",
    "coffee-script": "latest",
    "dracupid-no": "https://github.com/dracupid/dracupid-no.git",
