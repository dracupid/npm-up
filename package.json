  "version": "2.7.0",
  "description": "Check the latest versions of dependencies on npm gracefully, and do whatever you want.",
  "main": "./dist/npm-up.js",
  "bin": {
    "npm-up": "./bin/bin.js"
  },
  "nofile": {
